{
  "name": "kombu",
  "version": "0.1.0",
  "description": "A font format converter",
  "main": "dist/index.js",
  "scripts": {
    "build": "yarn make-wasm && yarn ts && yarn copy-wasm && yarn webpack && yarn html",
    "make-wasm": "emmake make",
    "ts": "tsc",
    "ts-watch": "tsc -w",
    "html": "node build/html.js",
    "copy-wasm": "cp dist/ffi.wasm public/",
    "test": "ava -v test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bashi/kombu.git"
  },
  "keywords": [
    "woff2",
    "opentype"
  ],
  "author": "Kenichi Ishibashi <ishibashi.kenichi@gmail.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "ava": "^0.25.0",
    "dotenv": "^6.0.0",
    "typescript": "^2.9.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.3"
  },
  "dependencies": {
    "zlibjs": "^0.3.1"
  }
}
